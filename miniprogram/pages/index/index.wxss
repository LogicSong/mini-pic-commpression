/**index.wxss**/
page {
  background-color: #f5f5f5;
}
.container {
  position: relative;
  display: flex;
  flex-direction: column;
  /* background-color: #eee; */
  align-items: center;
}
@font-face {
  font-family: "iconfont";
  src: url("data:font/woff2;charset=utf-8;base64,")
      format("woff2"),
    url("data:font/woff;charset=utf-8;base64,")
      format("woff"),
    url("iconfont.ttf") format("truetype");
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 100rpx;
  color: skyblue;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-imageload:before {
  content: "\e6f5";
}
.image {
  position: relative;
  margin-top: 20px;
  width: 250px;
  height: 300px;
  background-color: #fff;
  box-shadow: 0 0 15px 0px #ddd;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

#compressCanvasId {
  position: absolute;
  left: 0;
  top: 1000px;
}

.reload-image-text {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 12px;
  color: #aaa;
}

.image-info {
}

.compress-radio {
  margin-top: 15px;
  width: 250px;
}

.compress-radio > .compress-radio-title {
  font-size: 12px;
  color: #aaa;
}

.compress-radio-slider > slider {
  margin: 3px 5px;
}
